@charset "utf-8";


@font-face {
font-family:ng;
src:url(http://withmbc.imbc.com/common/css/font/NanumGothic.eot); /* IE9 Compat Modes */
src:url(http://withmbc.imbc.com/common/css/font/NanumGothic.eot?#iefix) format('embedded-opentype'), /* IE6-IE8 */
	url(http://withmbc.imbc.com/common/css/font/NanumGothic.woff) format('woff'), /* Modern Browsers */
	url(http://withmbc.imbc.com/common/css/font/NanumGothic.ttf)  format('truetype'); /* Safari, Android, iOS */
}

body{-webkit-text-size-adjust:none;line-height:1.428571429}
body{margin:0;padding:0;border:0;font-size:75%;font-family:sans-serif}

html,body,div,span,object,iframe,
h1,h2,h3,h4,h5,h6,p,blockquote,
pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,
img,ins,kbd,q,samp,small,strike,strong,sub,sup,tt,
var,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table
,caption,tbody,tfoot,thead,tr,th,td,input,textarea
,button,select{margin:0;padding:0;}

th,td{vertical-align:middle}
input,textarea,file,select{vertical-align:middle}
table{border-collapse:collapse;border:0;table-layout:fixed}
img,fieldset,iframe{border:0}
dl,ul,ol,li{list-style:none}
a{text-decoration:none}
em,cite,address,dfn,var{font-style:normal}




